<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="@color/badge_ranking_gradient_end" android:endColor="@color/badge_ranking_gradient_start" android:angle="0"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>
    <item android:left="1dp" android:top="1dp" android:right="1dp" android:bottom="1dp">
        <shape android:shape="rectangle">
            <gradient android:startColor="#f5f2f9" android:endColor="#c1cffd" android:angle="180"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>
</layer-list>
